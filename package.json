{
  "name": "clrfund-contracts",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "hardhat": "npx hardhat",
    "hardhat:local": "npx hardhat run --network localhost --no-compile",
    "test": "npx hardhat test",
    "start": "npx hardhat node",
    "build": "npx hardhat compile",
    "ts": "yarn run ts-node",
    "node": "hardhat node --port 18545",
    "deploy:local": "yarn clean && yarn build && hardhat run --network localhost scripts/deploy.ts",
    "deployTestRound:local": "hardhat run --network localhost scripts/deployTestRound.ts",
    "contribute:local": "hardhat run --network localhost scripts/contribute.ts",
    "vote:local": "hardhat run --network localhost scripts/vote.ts",
    "tally:local": "hardhat run --network localhost scripts/tally.ts",
    "finalize:local": "hardhat run --network localhost scripts/finalize.ts",
    "claim:local": "hardhat run --network localhost scripts/claim.ts",
    "clean": "rm -rf cache && rm -rf build"
  },
  "devDependencies": {
    "@kleros/gtcr-encoder": "^1.4.0",
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@typechain/ethers-v5": "^7.0.1",
    "@typechain/hardhat": "^2.3.0",
    "@types/chai": "^4.2.21",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.4.13",
    "chai": "^4.3.4",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.4.4",
    "hardhat": "^2.6.0",
    "ipfs-only-hash": "^4.0.0",
    "prettier": "^2.3.2",
    "prettier-plugin-solidity": "^1.0.0-beta.17",
    "ts-generator": "^0.1.1",
    "ts-node": "^10.2.0",
    "typechain": "^5.1.2",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@openzeppelin/contracts": "3.2.0",
    "maci-cli": "^0.9.4",
    "maci-contracts": "^0.9.4",
    "maci-crypto": "^0.9.1",
    "maci-domainobjs": "^0.9.1",
    "solidity-rlp": "^2.0.5"
  }
}
